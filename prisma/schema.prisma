// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model S1_stationary_comsbution {
  id                  Int                             @id @default(autoincrement())
  fuel_type_id        Int
  fuel_subtype        String                          @db.VarChar(255)
  heat_content_value  Float?
  co2_emission_factor Float
  ch4_emission_factor Float
  n2o_emission_factor Float
  fuel_type           S1_stationary_comsbution_lookup @relation(fields: [fuel_type_id], references: [fuel_type_id])
}

model S1_stationary_comsbution_lookup {
  fuel_type_id                Int                        @id
  fuel_type                   String                     @db.VarChar(255)
  heat_content_unit           String?                    @db.VarChar(255)
  co2_emission_unit           String                     @db.VarChar(255)
  ch4_emission_unit           String                     @db.VarChar(255)
  n2o_emission_unit           String                     @db.VarChar(255)
  Scope1_StationaryComsbution S1_stationary_comsbution[]
}

model S1_mobile_combustion {
  id              Int    @id @default(autoincrement())
  fuel_type       String
  emission_factor Float
  emission_unit   String
}
