generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

type StCombusHcEmisCh4 {
  emisUnit  String
  emisValue Float
}

type StCombusHcEmisCo2 {
  emisUnit  String
  emisValue Float
}

type StCombusHcEmisN2O {
  emisUnit  String
  emisValue Float
}

model st_combus_hc {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  fuelSubtype String
  fuelType    String
  hcUnit      String
  hcValue     Float
}

model st_combus_hc_emis {
  id          String            @id @default(auto()) @map("_id") @db.ObjectId
  CH4         StCombusHcEmisCh4
  CO2         StCombusHcEmisCo2
  N2O         StCombusHcEmisN2O
  fuelSubtype String
  fuelType    String
}
