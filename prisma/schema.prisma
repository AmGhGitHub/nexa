generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

type StCombusHcEmisCh4 {
  emisUnit  String
  emisValue Float
}

type StCombusHcEmisCo2 {
  emisUnit  String
  /// Multiple data types found: Float: 95.2%, Int: 4.8% out of 63 sampled entries
  emisValue Json
}

type StCombusHcEmisN2O {
  emisUnit  String
  emisValue Float
}

model st_combus_hc_emis {
  id          String            @id @default(auto()) @map("_id") @db.ObjectId
  fuelType    String
  fuelSubtype String
  CH4         StCombusHcEmisCh4
  CO2         StCombusHcEmisCo2
  N2O         StCombusHcEmisN2O
}

model st_combus_heat_content {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  fuelSubType String
  fuelType    String
  /// Multiple data types found: Float: 93.1%, Int: 6.9% out of 58 sampled entries
  heatContent Json
  unit        String
}
